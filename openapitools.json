{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.1.0",
    "storageDir": "target/generated-sources/openapi",
    "generators": {
      "v3.0": {
        "generatorName": "typescript",
        "output": "#{cwd}/target/generated-sources/sdk/",
        "glob": "target/generated-sources/pe-api-combined-0.0.0.yaml",
        "additionalProperties": {
          "npmName": "restClient",
          "supportsES6": true,
          "npmVersion": "6.9.0",
          "withInterfaces": true
        }
      }
    }
  }
}
